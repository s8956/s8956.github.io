[asterisk]

enabled             = true
backend             = polling
filter              = %(__name__)s
logpath             = /var/log/%(__name__)s/messages.log
action              = nftables-allports[name="%(__name__)s-tcp", protocol="tcp"]
                      nftables-allports[name="%(__name__)s-udp", protocol="udp"]
maxretry            = 10
